{
    "name": "uatthaphon/package-name",
    "description": "A package starter kits.",
    "keywords": [
        "laravel",
        "package"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Atthaphon Urairat",
            "email": "u.atthaphon@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Uatthaphon\\PackageName\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/phpunit/phpunit/phpunit",
        "testUnit": "@test --testsuite Unit",
        "testFeature": "@test --testsuite Feature",
        "testVerbose": "@test -vvv"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uatthaphon\\PackageName\\PackageNameServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}